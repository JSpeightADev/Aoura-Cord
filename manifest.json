{
"update_url": "https://clients2.google.com/service/update2/crx",
"manifest_version":3,
"minimum_chrome_version":"91",
"name":"Aouracord",
"description":"Discord Customizer Mod",
"author":"Aoura",
"homepage_url":"https://aouracord.github.io",
"icons":{"128":"icon.png"},
"host_permissions":["*://*.discord.com/*",
"https://raw.githubusercontent.com/*"],
"permissions":["declarativeNetRequest"],
"content_scripts":[{"run_at":"document_start",
"matches":["*://*.discord.com/*"],
"js":["content.js"],"all_frames":true}],
"web_accessible_resources":[{"resources":["dist/*","third-party/*"],
"matches":["*://*.discord.com/*"]}],
"declarative_net_request":{"rule_resources":[{"id":"modifyResponseHeaders",
"enabled":true,
"path":"modifyResponseHeaders.json"}]},
"version":"1.0.0"
}